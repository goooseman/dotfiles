server {
    listen 80;
    server_name plex.raspberrypi.local;
    location / {
        proxy_set_header Host $host;
        proxy_pass http://127.0.0.1:32400;
        proxy_redirect off;
    }
}

server {
    listen 80;
    server_name plex-tools.raspberrypi.local;
    location / {
        proxy_set_header Host $host;
        proxy_pass http://127.0.0.1:33400;
        proxy_redirect off;
    }
}